{
  "name": "frosted-ui",
  "version": "0.0.0",
  "description": "",
  "private": false,
  "main": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "build": "concurrently \"pnpm run build:tsc\" \"pnpm run build:swc\"",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:swc": "concurrently \"pnpm run build:swc:components\" \"pnpm run build:swc:lib\" \"pnpm run build:swc:index\"",
    "build:swc:components": "swc components -d components",
    "build:swc:lib": "swc lib -d lib",
    "build:swc:index": "swc index.ts -o index.js",
    "dev": "concurrently \"pnpm run build:tsc --watch\" \"pnpm run dev:swc\"",
    "dev:swc": "concurrently \"pnpm run build:swc:components --watch\" \"pnpm run build:swc:lib --watch\" \"pnpm run build:swc:index --watch\"",
    "release": "turbo-module publish",
    "vite-dev": "vite",
    "vite-build": "tsc && vite build",
    "vite-preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-vite": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@swc/cli": "^0.1.60",
    "@swc/core": "^1.3.32",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@whop-sdk/turbo-module": "0.0.4",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.27",
    "tsconfig": "workspace:^0.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.1"
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.2.0",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tsconfig": "workspace:^0.0.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": ">=18.0.0"
  }
}
